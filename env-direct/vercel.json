{
  "version": 2,
  "name": "environment-direct-frontend",
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "env": {
    "VITE_API_URL": "https://env-backend-a73f7074660a.herokuapp.com"
  },
  "build": {
    "env": {
      "VITE_API_URL": "https://env-backend-a73f7074660a.herokuapp.com"
    }
  },
  "rewrites": [
    {
      "source": "/geojson/(.*)",
      "destination": "/geojson/$1"
    },
    {
      "source": "/(.*)\\.geojson",
      "destination": "/$1.geojson"
    },
    {
      "source": "/((?!api|geojson|img|fonts|models|cesium|_next|static|\\.geojson|\\.json|\\.js|\\.css|\\.png|\\.jpg|\\.jpeg|\\.gif|\\.svg|\\.ico|\\.woff|\\.woff2|\\.ttf|\\.eot).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)\\.geojson",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    },
    {
      "source": "/geojson/(.*).geojson",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
} 